<view class="y_talk-boxz">
<!-- 外窗结束 -->
	<view class="y-talk-box-title" style="background:{{wheel_demo}};">
		<text id="lovekfcompanytitle">{{maintitle}}</text>
		<text>{{littletitle}}</text>
    		<image src="data:image/png;base64," class="y-talk-box-title-img {{phoneswitch == 1? '':'phoneswitchhide'}}" bindtap="tel"></image>
    <image src="data:image/png;base64," class="y-talk-box-title-a" bindtap="navigator_back"></image>
		<navigator href="tel:0147-88469258" class="y-talk-box-title-phone-a" id="img_phonecall"></navigator>	
	</view>
	<view class="y-talk-box-content clearfix">
		<view class="y-talk-box-content-left">
      <!-- o.scrollTop = o.scrollHeight; -->
			<scroll-view scroll-y="true" scroll-top="{{scrollTop}}" class="y-talk-box-content-left-top" id="y-talk-box-content-left-top" style='height:{{windowHeight-220}}rpx;'>
        <view class="container_innerHeight">
          <text class="y-talk-box-content-left-topi {{loadhistoryisshow ? '' :'loadhistoryhide'}}" bindtap="loadhistoryMessage">加载历史消息</text>
          <!-- 百度搜索开始 -->
          <!-- 百度搜索结束 -->
          <!-- 天气信息 开始-->
          <!-- 天气信息结束-->
          <!-- 中间对话区域对话遍历开始 -->
          <view wx:for-items="{{array}}">
              <!-- 人类说话开始 -->
              <view class="customer-sen clearfix" wx:if="{{item.typeId ==0}}">
                <!-- <image src="../../pics/touxiang1.png"></image> -->
                <image src="{{userInfo.avatarUrl}}"></image>
                <view>
                  {{item.message}}
                  <image src="data:image/png;base64,"></image>
                </view>
              </view>
              <!-- 人类说话结束 -->
              <!-- 机器人说话开始 -->
              <view class="robot-sen clearfix" wx:if="{{item.typeId == -1}}">
                <image src="data:image/png;base64,"></image>
                <view>
                  {{item.message}}
                  <image src="data:image/png;base64,"></image>
                </view>
              </view>
              <!-- 机器人说话结束 -->
              <!-- 机器人发图片开始 -->
              <view class="robot-sen clearfix" wx:if="{{item.typeId == 2}}">
                <image src="data:image/png;base64,"></image>
                <view>
                  <image src="{{item.imageURL}}" class="img-robot"></image>
                  <image src="data:image/png;base64,"></image>
                </view>
              </view>
              <!-- 机器人发图片结束 -->
              <!-- 机器人回答作诗开始 -->
              <view class="robot-sen clearfix" wx:if="{{item.typeId == 7}}">
                  <image src="data:image/png;base64,"></image>
                  <view>
                    {{item.message}}
                    <image src="data:image/png;base64,"></image>
                  </view>
              </view>
              <!-- 机器人回答作诗结束 -->
          </view>
          <!-- 中间对话区域对话遍历结束 -->
        </view>
			</scroll-view>
      <!-- 语音说话时动画 -->
      <view  wx:if="{{isSpeaking}}"  class="speak-style">
          <image class="sound-style" src="data:image/png;base64,"></image>
          <image wx:if="{{j==2}}" class="sound-style" src="data:image/png;base64,"></image>
          <image wx:if="{{j==3}}" class="sound-style" src="data:image/png;base64,"></image>
          <image wx:if="{{j==4}}" class="sound-style" src="data:image/png;base64,"></image>
          <image wx:if="{{j==5}}" class="sound-style" src="data:image/png;base64,"></image>
      </view>
      <!-- 语音输入 -->
      <view class="y-talk-box-content-left-bottom {{voiceorwrite ? '':'y_voice_insert_none'}}">
          <image src="data:image/png;base64," title="转人工" class="voicetowriteimg" bindtap='voicetowrite'></image>
          <button class="voice_tabbtn {{isSpeaking?'word-deBtn1':''}}" bindtouchstart="touchdown" bindtouchend="touchup">按住说话</button>
      </view>
      <!-- 文字输入 -->
			<view class="y-talk-box-content-left-bottom {{voiceorwrite ? 'y-talk-box-content-left-bottom_none':''}}">
				<image src="data:image/png;base64," title="转人工" class="y-talk-box-content-left-bottom-voice" bindtap='writetovoice'></image>
				<textarea  class="y-talk-box-content-left-bottom-text" value="{{inputValue}}" name="shuru" bindinput='catchMessage' bindconfirm="submitMessage" cursor-spacing='20'></textarea>
        <button value="发送" class="y-talk-box-content-left-bottom-input" bindtap="sendMessage">发送</button>
        <!-- 联想区域 -->
        <view  class="textThink {{textThinkIsShow ? '':'textThink_hide'}}">
            <input wx:for-items="{{arrayThink}}" bindtap='thinktab' value="{{item.issueOld}}" data-name="{{item.issueOld}}"></input>
        </view>
				<!-- <input type="button" value="发送" class="y-talk-box-content-left-bottom-input" onclick="yksend()"/> -->
			</view> 
		</view>
	</view>
</view>
