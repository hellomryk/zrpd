/* pages/mxjb/mxjb.wxss */
@import '/pages/index/index.wxss';
.y_talk-boxz {
  height:850rpx;
}
.y-talk-box-content-left-top {
  height:850rpx;
}
.slide_select {
  position:fixed;
  bottom:0;
  left:0;
  z-index:99999;
  width:100%;
  height:364rpx;
}
.slide_select_confim {
  width:100%;
  height:92rpx;
  text-align: center; 
  background:#15c86c;
  color:#fff;
  line-height: 92rpx;
  font-size:28rpx;
}
.y-talk-box-content-left-top {
  background:#f5f5f5;
}
.slide_select {
  background:none;
}
picker-view-column {
  background: none;
}

.robot-sen {
  margin-bottom:30rpx;
}
.starttest {
  font-size:28rpx;
  line-height:79rpx;
  color:#999999;
  padding-left:34rpx;
}
.startborder {
  border-top:1rpx solid #e7e7e7;
  border-bottom:1rpx solid #e7e7e7;
  width:446rpx;
  margin-top:20rpx;
}
.robot-sen>.mxjb_robot_public {
  background:#fff;
  max-width:412rpx;
  font-size:28rpx;
  line-height:44rpx;
  padding:20rpx 34rpx;
  border-radius:30rpx;
  color:#323232;
}
.robot-sen>.mxjb_robot_starttest {
    background:#fff;
  max-width:480rpx;
  font-size:28rpx;
  line-height:44rpx;
  border-radius:30rpx;
  color:#323232;
  padding:20rpx 0;
}
.mxjb_robot_starttest_text {
  margin:0 34rpx;
  display:block;
}
.mxjb_people {
  float:right;
  margin-right:20rpx;
}
.mxjb_people>.mxjb_robot_public {
  background:#15c86c;
  color:#fff;
}
.mxjb_text_write {
  position:fixed;
  left:0;
  bottom:0;
  width:100%;
}
.mxjb_text_write_input {
  font-size:30rpx;
  height:104rpx;
  background:#fff;
  line-height:104rpx;
  text-indent:30rpx;
}
.starttestlistborder {
  border-bottom: none;
}